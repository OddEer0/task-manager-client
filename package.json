{
	"name": "eer0-task-manager-client",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"lint": "eslint ./**/*{.ts,.tsx,.js,.jsx}",
		"lint:fix": "eslint ./**/*{.ts,.tsx,.js,.jsx} --fix",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"stylelint": "stylelint src/**/*{.css,.scss}",
		"stylelint:fix": "stylelint src/**/*{.css,.scss,.ts,.tsx,.js,.jsx} --fix",
		"prettier": "prettier --check src/**/*{.css,.scss,.ts,.tsx,.js,.jsx}",
		"prettier:fix": "prettier --write src/**/*{.css,.scss,.ts,.tsx,.js,.jsx}",
		"preview": "vite preview",
		"prepare": "husky install"
	},
	"dependencies": {
		"@chakra-ui/react": "^2.7.0",
		"@emotion/react": "^11.11.0",
		"@emotion/styled": "^11.11.0",
		"@uiw/react-color-chrome": "^1.3.1",
		"effector": "^22.8.6",
		"effector-react": "^22.5.3",
		"effector-storage": "^6.0.1",
		"framer-motion": "^10.12.16",
		"normalize.css": "^8.0.1",
		"react": "^18.2.0",
		"react-dnd": "^16.0.1",
		"react-dnd-html5-backend": "^16.0.1",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.44.3",
		"react-icons": "^4.9.0",
		"react-router-dom": "^6.11.2"
	},
	"devDependencies": {
		"@effector/swc-plugin": "^0.0.10",
		"@swc/core": "^1.3.64",
		"@trivago/prettier-plugin-sort-imports": "^4.1.1",
		"@types/enzyme": "^3.10.13",
		"@types/jest": "^29.5.2",
		"@types/react": "^18.0.37",
		"@types/react-dom": "^18.0.11",
		"@typescript-eslint/eslint-plugin": "^5.59.0",
		"@typescript-eslint/parser": "^5.59.0",
		"@vitejs/plugin-react-swc": "^3.0.0",
		"effector-logger": "^0.14.0",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.7",
		"eslint": "8.2.0",
		"eslint-config-airbnb": "19.0.4",
		"eslint-plugin-import": "2.25.3",
		"eslint-plugin-jsx-a11y": "6.5.1",
		"eslint-plugin-react": "7.28.0",
		"eslint-plugin-react-hooks": "4.3.0",
		"eslint-plugin-react-refresh": "^0.3.4",
		"jest": "^29.5.0",
		"prettier": "^2.8.8",
		"sass": "^1.62.1",
		"stylelint": "^15.6.3",
		"stylelint-config-standard-scss": "^9.0.0",
		"typescript": "^5.1.3",
		"vite": "^4.3.9",
		"vitest": "^0.31.4",
		"husky": "^8.0.0"
	}
}
